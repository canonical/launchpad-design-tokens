{
  "lp": {
    "typography": {
      "paragraph": {
        "$type": "typography",
        "xs": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.xs}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.xs}"
          }
        },
        "xs-strong": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.xs}",
            "fontWeight": "{lp.typography.weight.bold}",
            "lineHeight": "{lp.typography.lineHeight.xs}"
          }
        },
        "s": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.s}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.s}"
          }
        },
        "s-strong": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.s}",
            "fontWeight": "{lp.typography.weight.bold}",
            "lineHeight": "{lp.typography.lineHeight.s}"
          }
        },
        "default": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.m}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.m}"
          }
        },
        "default-strong": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.m}",
            "fontWeight": "{lp.typography.weight.bold}",
            "lineHeight": "{lp.typography.lineHeight.m}"
          }
        },
        "default-smallCaps": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.default}",
            "fontSize": "{lp.typography.fontSize.m}",
            "fontWeight": "{lp.typography.weight.bold}",
            "lineHeight": "{lp.typography.lineHeight.m}",
            "letterSpacing": "{lp.typography.letterSpacing.l}",
            "fontVariant": "small-caps"
          },
          "$description": "`letter-spacing` in CSS is not a constituent of `font` and as such it is skipped in the output of this composite token. Be sure to apply `lp.typography.letterSpacing.l` separately whenever design requires it."
        }
      },
      "code": {
        "$type": "typography",
        "xs": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.mono}",
            "fontSize": "{lp.typography.fontSize.xs}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.xs}"
          }
        },
        "s": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.mono}",
            "fontSize": "{lp.typography.fontSize.s}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.s}"
          }
        },
        "default": {
          "$value": {
            "fontFamily": "{lp.typography.fontFamily.mono}",
            "fontSize": "{lp.typography.fontSize.m}",
            "fontWeight": "{lp.typography.weight.regular}",
            "lineHeight": "{lp.typography.lineHeight.m}"
          }
        }
      },
      "h1": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xxl}",
          "fontWeight": "{lp.typography.weight.medium}",
          "lineHeight": "{lp.typography.lineHeight.xxl}"
        }
      },
      "h2": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xxl}",
          "fontWeight": "{lp.typography.weight.extraThin}",
          "lineHeight": "{lp.typography.lineHeight.xxl}"
        }
      },
      "h3": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xl}",
          "fontWeight": "{lp.typography.weight.medium}",
          "lineHeight": "{lp.typography.lineHeight.xl}"
        }
      },
      "h4": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xl}",
          "fontWeight": "{lp.typography.weight.thin}",
          "lineHeight": "{lp.typography.lineHeight.xl}"
        }
      },
      "h5": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.l}",
          "fontWeight": "{lp.typography.weight.medium}",
          "lineHeight": "{lp.typography.lineHeight.l}"
        }
      },
      "h6": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.l}",
          "fontWeight": "{lp.typography.weight.light}",
          "lineHeight": "{lp.typography.lineHeight.l}"
        }
      },
      "weight": {
        "$type": "fontWeight",
        "extraThin": {
          "$value": 180
        },
        "thin": {
          "$value": 200
        },
        "light": {
          "$value": 300
        },
        "regular": {
          "$value": 400
        },
        "medium": {
          "$value": 450
        },
        "bold": {
          "$value": 550
        }
      },
      "letterSpacing": {
        "$type": "dimension",
        "m": {
          "$value": {
            "value": 0,
            "unit": "em"
          }
        },
        "l": {
          "$value": {
            "value": 0.05,
            "unit": "em"
          }
        },
        "xl": {
          "$value": {
            "value": 0.07,
            "unit": "em"
          }
        }
      },
      "fontFamily": {
        "$type": "fontFamily",
        "default": {
          "$value": [
            "Ubuntu Sans",
            "Ubuntu",
            "-apple-system",
            "Segoe UI",
            "Roboto",
            "Oxygen",
            "Cantarell",
            "Fira Sans",
            "Droid Sans",
            "Helvetica Neue",
            "sans-serif"
          ]
        },
        "mono": {
          "$value": [
            "Ubuntu Sans Mono",
            "Ubuntu Mono",
            "Consolas",
            "Monaco",
            "Courier",
            "monospace"
          ]
        }
      }
    }
  }
}
