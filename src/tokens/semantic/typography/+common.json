{
  "tmp": {
    "typography": {
      "paragraph": {
        "$type": "typography",
        "xs": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.default}",
            "fontSize": "{tmp.typography.fontSize.xs}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.xs}"
          }
        },
        "s": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.default}",
            "fontSize": "{tmp.typography.fontSize.s}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.s}"
          }
        },
        "default": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.default}",
            "fontSize": "{tmp.typography.fontSize.m}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.m}"
          }
        }
      },
      "code": {
        "$type": "typography",
        "xs": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.mono}",
            "fontSize": "{tmp.typography.fontSize.xs}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.xs}"
          }
        },
        "s": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.mono}",
            "fontSize": "{tmp.typography.fontSize.s}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.s}"
          }
        },
        "default": {
          "$value": {
            "fontFamily": "{tmp.typography.fontFamily.mono}",
            "fontSize": "{tmp.typography.fontSize.m}",
            "fontWeight": "{tmp.typography.weight.400}",
            "lineHeight": "{tmp.typography.lineHeight.m}"
          }
        }
      },
      "h3": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{tmp.typography.fontFamily.default}",
          "fontSize": "{tmp.typography.fontSize.xl}",
          "fontWeight": "{tmp.typography.weight.450}",
          "lineHeight": "{tmp.typography.lineHeight.l}"
        }
      },
      "h4": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{tmp.typography.fontFamily.default}",
          "fontSize": "{tmp.typography.fontSize.xl}",
          "fontWeight": "{tmp.typography.weight.200}",
          "lineHeight": "{tmp.typography.lineHeight.l}"
        }
      },
      "h5": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{tmp.typography.fontFamily.default}",
          "fontSize": "{tmp.typography.fontSize.l}",
          "fontWeight": "{tmp.typography.weight.450}",
          "lineHeight": "{tmp.typography.lineHeight.l}"
        }
      },
      "h6": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{tmp.typography.fontFamily.default}",
          "fontSize": "{tmp.typography.fontSize.l}",
          "fontWeight": "{tmp.typography.weight.300}",
          "lineHeight": "{tmp.typography.lineHeight.l}"
        }
      },
      "weight": {
        "$type": "fontWeight",
        "180": {
          "$value": 180
        },
        "200": {
          "$value": 200
        },
        "300": {
          "$value": 300
        },
        "400": {
          "$value": 400
        },
        "450": {
          "$value": 450
        }
      },
      "letterSpacing": {
        "$type": "dimension",
        "m": {
          "$value": {
            "value": 0,
            "unit": "em"
          }
        },
        "l": {
          "$value": {
            "value": 0.05,
            "unit": "em"
          }
        },
        "xl": {
          "$value": {
            "value": 0.07,
            "unit": "em"
          }
        }
      },
      "fontFamily": {
        "$type": "fontFamily",
        "default": {
          "$value": [
            "Ubuntu Sans",
            "Ubuntu",
            "-apple-system",
            "Segoe UI",
            "Roboto",
            "Oxygen",
            "Cantarell",
            "Fira Sans",
            "Droid Sans",
            "Helvetica Neue",
            "sans-serif"
          ]
        },
        "mono": {
          "$value": [
            "Ubuntu Sans Mono",
            "Ubuntu Mono",
            "Consolas",
            "Monaco",
            "Courier",
            "monospace"
          ]
        }
      }
    }
  }
}
