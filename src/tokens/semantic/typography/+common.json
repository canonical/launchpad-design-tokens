{
  "tmp": {
    "typography": {
      "weight": {
        "$type": "fontWeight",
        "180": {
          "$value": 180
        },
        "200": {
          "$value": 200
        },
        "300": {
          "$value": 300
        },
        "400": {
          "$value": 400
        },
        "450": {
          "$value": 450
        }
      },
      "letterSpacing": {
        "$type": "dimension",
        "medium": {
          "$value": {
            "value": 0,
            "unit": "em"
          }
        },
        "large": {
          "$value": {
            "value": 0.05,
            "unit": "em"
          }
        },
        "xLarge": {
          "$value": {
            "value": 0.07,
            "unit": "em"
          }
        }
      },
      "fontFamily": {
        "$type": "fontFamily",
        "default": {
          "$value": [
            "Ubuntu Sans",
            "Ubuntu",
            "-apple-system",
            "Segoe UI",
            "Roboto",
            "Oxygen",
            "Cantarell",
            "Fira Sans",
            "Droid Sans",
            "Helvetica Neue",
            "sans-serif"
          ]
        },
        "mono": {
          "$value": [
            "Ubuntu Sans Mono",
            "Ubuntu Mono",
            "Consolas",
            "Monaco",
            "Courier",
            "monospace"
          ]
        }
      }
    }
  }
}
