          "$type": "dimension"
        },
        "s": {
          "$value": "14px",
          "$type": "dimension"
        },
        "m": {
          "$value": "16px",
          "$type": "dimension"
        },
        "l": {
          "$value": "18px",
          "$type": "dimension"
        },
        "xl": {
          "$value": "24px",
          "$type": "dimension"
        },
        "xxl": {
          "$value": "32px",
          "$type": "dimension"
        }
      },
      "lineHeight": {
        "xs": {
          "$value": "16px",
          "$type": "dimension"
        },
        "s": {
          "$value": "20px",
          "$type": "dimension"
        },
        "m": {
          "$value": "24px",
          "$type": "dimension"
        },
        "l": {
          "$value": "24px",
          "$type": "dimension"
        },
        "xl": {
          "$value": "32px",
          "$type": "dimension"
        },
        "xxl": {
          "$value": "40px",
          "$type": "dimension"
        }
      }
    }
  }
}
