          "fontSize": "{lp.typography.fontSize.xxl}",
          "fontWeight": "{lp.typography.weight.extraThin}",
          "lineHeight": "{lp.typography.lineHeight.xxl}"
        },
        "$type": "typography"
      },
      "h3": {
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xl}",
          "fontWeight": "{lp.typography.weight.medium}",
          "lineHeight": "{lp.typography.lineHeight.xl}"
        },
        "$type": "typography"
      },
      "h4": {
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.xl}",
          "fontWeight": "{lp.typography.weight.thin}",
          "lineHeight": "{lp.typography.lineHeight.xl}"
        },
        "$type": "typography"
      },
      "h5": {
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.l}",
          "fontWeight": "{lp.typography.weight.medium}",
          "lineHeight": "{lp.typography.lineHeight.l}"
        },
        "$type": "typography"
      },
      "h6": {
        "$value": {
          "fontFamily": "{lp.typography.fontFamily.default}",
          "fontSize": "{lp.typography.fontSize.l}",
          "fontWeight": "{lp.typography.weight.light}",
          "lineHeight": "{lp.typography.lineHeight.l}"
        },
        "$type": "typography"
      },
      "weight": {
        "extraThin": {
          "$value": 180,
          "$type": "fontWeight"
        },
        "thin": {
          "$value": 200,
          "$type": "fontWeight"
        },
        "light": {
          "$value": 300,
          "$type": "fontWeight"
        },
        "regular": {
          "$value": 400,
          "$type": "fontWeight"
        },
        "medium": {
          "$value": 450,
          "$type": "fontWeight"
        },
        "bold": {
          "$value": 550,
          "$type": "fontWeight"
        }
      },
      "letterSpacing": {
        "m": {
          "$value": "0em",
          "$type": "dimension"
        },
        "l": {
          "$value": "0.05em",
          "$type": "dimension"
        },
        "xl": {
          "$value": "0.07em",
          "$type": "dimension"
        }
      },
      "fontFamily": {
        "default": {
          "$value": [
            "Ubuntu Sans",
            "Ubuntu",
            "-apple-system",
            "Segoe UI",
            "Roboto",
            "Oxygen",
            "Cantarell",
            "Fira Sans",
            "Droid Sans",
            "Helvetica Neue",
            "sans-serif"
          ],
          "$type": "fontFamily"
        },
        "mono": {
          "$value": [
            "Ubuntu Sans Mono",
            "Ubuntu Mono",
            "Consolas",
            "Monaco",
            "Courier",
            "monospace"
          ],
          "$type": "fontFamily"
        }
      },
      "fontSize": {
        "xs": {
